   "source": [
    "import numpy as np\n",
    "def calculate_matrix_mean(matrix: list[list[float]], mode: str) -> list[float]:\n",
    "\tif mode == 'row':\n",
    "\t\tmeans = [sum(row)/len(row) for row in matrix]\n",
    "\t\n",
    "\tif mode == \"column\":\n",
    "\t\tmeans = [sum(col)/len(col) for col in zip(*matrix)]\n",
    "\n",
    "\treturn means"
   ]
